        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum by (method) (rate(grpc_requests_total[5m]))",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "title": "Request Rate by Method",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "id": 2,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, method) (rate(grpc_request_duration_seconds_bucket[5m])))",
          "legendFormat": "{{method}} p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, method) (rate(grpc_request_duration_seconds_bucket[5m])))",
          "legendFormat": "{{method}} p99",
          "refId": "B"
        }
      ],
      "title": "Request Latency (p95/p99) by Method",
      "type": "timeseries"
    }
